import java.util.Scanner;
public class TicketDay {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter day (weekday/weekend): ");
        String day = sc.next();

        if (day.equalsIgnoreCase("weekday"))
            System.out.println("Weekday price");
        else System.out.println("Weekend price");
    }
}
